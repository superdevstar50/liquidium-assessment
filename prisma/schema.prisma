generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Offer {
  id        String @id @default(cuid())
  amount    Float
  term      Int
  interest  Float
  ordinalId String @map("ordinal_id")
}
